[alias]
  uncommit = reset --soft HEAD^
  tracked = ls-tree --name-only --full-tree -r HEAD
  contrib = shortlog --summary --email
  untrack = rm --cached
  amend = commit --ammend --no-edit
  commits = log origin.. --oneline
[pull]
  rebase = true
